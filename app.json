{
  "expo": {
    "name": "alma",
    "slug": "alma",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/small-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/small-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#030637"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.willferns2000.alma"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/small-logo.png",
        "backgroundColor": "#030637"
      },
      "package": "com.willferns2000.alma"
    },
    "web": {
      "favicon": "./assets/logo-without-text.png"
    },
    "extra": {
      "eas": {
        "projectId": "0666f15f-e663-423d-b47f-ceb02608cd80"
      }
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
